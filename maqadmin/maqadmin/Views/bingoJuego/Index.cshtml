@model IEnumerable<maqadmin.Models.bingoJuego>

@{
    ViewBag.Title = "Index";
}

@*<h2>Index</h2>*@

<p>
    @Html.ActionLink("Crear nuevo Juego", "Create")
</p>
<table>
    <tr>
        <th>
            Id Local
        </th>
        <th>
            Fecha-Hora inicio
        </th>
        <th>
            Fecha-Hora fin
        </th>      
       @* <th>
            Estado juego
        </th>*@
        <th>
            Premio del juego
        </th>
        <th>
            Espera entre bolitas(seg.)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idlocal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.inicioJuego)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.finjuego)
        </td>
       @* <td>
            @Html.DisplayFor(modelItem => item.letrasnumeros)
        </td>*@
      @*  <td>
            @Html.DisplayFor(modelItem => item.estadoJuego)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.premio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.esperaNumeroSeg)
        </td>
       
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idbingo }) |
            @Html.ActionLink("Ver", "Details", new { id=item.idbingo }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idbingo })
        </td>
    </tr>
}

</table>
